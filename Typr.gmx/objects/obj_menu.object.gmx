var move = 0;
move -= max(keyboard_check_pressed(vk_up),0);
move += max(keyboard_check_pressed(vk_down),0);

if (move != 0)
{
    blinker = "|";
    alarm[0] = 0.53*room_speed;
    //Play sound
    audio_play_sound(obj_sound.menu_move,1,0);
    mpos += move;
    
    if (mpos &lt; 0)
    mpos = array_length_1d(menu) - 1;
    if (mpos &gt; array_length_1d(menu) - 1)
    mpos = 0;
}

var push;
push = max(keyboard_check_released(vk_enter),0)
if (push == 1 &amp;&amp; pushed == 0)
{
pushed = 1;
//Play sound
audio_play_sound(obj_sound.menu_push,1,0);
scr_menu();
}
</string>
